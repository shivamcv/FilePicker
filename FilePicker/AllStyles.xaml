    <SolidColorBrush x:Key="WhiteOpacity" Color="White" Opacity="0.4"></SolidColorBrush>
    <SolidColorBrush x:Key="WhiteOpacity2" Color="White" Opacity="0.2"></SolidColorBrush>



    <SolidColorBrush x:Key="MouseoverComboboxitem" Color="{StaticResource customBlue}" Opacity="0.1"/>
