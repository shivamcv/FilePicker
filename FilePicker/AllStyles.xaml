    <SolidColorBrush x:Key="BlueopacityBrush5" Color="{StaticResource customBlue}" Opacity="0.5"></SolidColorBrush>

    <SolidColorBrush x:Key="TileBlueopacityBrush" Color="{StaticResource customBlue}" Opacity="0.7"></SolidColorBrush>


    <SolidColorBrush x:Key="MasterDataOpacity" Color="{StaticResource customBlue}" Opacity="0.05"/>

    <SolidColorBrush x:Key="RedBrushOpacity" Color="{StaticResource customRed}" Opacity="0.3"></SolidColorBrush>
    <SolidColorBrush x:Key="RedBrushOpacity05" Color="{StaticResource customRed}" Opacity="0.05"></SolidColorBrush>
